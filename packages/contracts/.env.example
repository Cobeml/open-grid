# DEVELOPMENT OPTIONS
# ===================
REPORT_GAS=false
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_for_gas_reporting

# UTILITY API CONFIGURATION (for Chainlink Functions)
# ===================================================
UTILITY_API_KEY=your_utility_api_key
CON_EDISON_API_KEY=your_con_edison_api_key

# DEPLOYMENT PREFERENCES
# =====================
# Set to true to automatically verify contracts after deployment
AUTO_VERIFY=true

# Set deployment timeout in milliseconds
DEPLOYMENT_TIMEOUT=300000

# MULTI-CHAIN DEPLOYMENT
# ======================
# Comma-separated list of networks to deploy to (optional, defaults to all)
# DEPLOY_NETWORKS=polygon,arbitrum,optimism,base

# Set to true to continue deployment even if some networks fail
IGNORE_DEPLOYMENT_ERRORS=false

# ===========================================
# SETUP INSTRUCTIONS:
# ===========================================
# 1. Copy this file to .env in the same directory
# 2. Fill in your actual values (remove the placeholder text)
# 3. Get your Infura project ID from https://infura.io
# 4. Get API keys from respective block explorers
# 5. Set up Chainlink Functions subscriptions on each network
# 6. Fund your deployer account with native tokens for each network
# 7. Test deployment on testnets first before mainnet
# ===========================================