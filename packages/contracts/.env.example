ARBITRUM_DON_ID=0x66756e2d617262697472756d2d6d61696e6e65742d310000000000000000000
OPTIMISM_DON_ID=0x66756e2d6f7074696d69736d2d6d61696e6e65742d310000000000000000000
BASE_DON_ID=0x66756e2d626173652d6d61696e6e65742d31000000000000000000000000000
AVALANCHE_DON_ID=0x66756e2d6176616c616e6368652d6d61696e6e65742d310000000000000000
BNB_DON_ID=0x66756e2d626e622d6d61696e6e65742d3100000000000000000000000000000

# Testnet DON IDs
SEPOLIA_DON_ID=0x66756e2d657468657265756d2d7365706f6c69612d3100000000000000000000
