POLYGON_SUBSCRIPTION_ID=1
ARBITRUM_SUBSCRIPTION_ID=1
OPTIMISM_SUBSCRIPTION_ID=1
BASE_SUBSCRIPTION_ID=1
AVALANCHE_SUBSCRIPTION_ID=1
BNB_SUBSCRIPTION_ID=1
GNOSIS_SUBSCRIPTION_ID=1

# Testnet Subscription IDs
SEPOLIA_SUBSCRIPTION_ID=1
POLYGON_MUMBAI_SUBSCRIPTION_ID=1
ARBITRUM_GOERLI_SUBSCRIPTION_ID=1
OPTIMISM_GOERLI_SUBSCRIPTION_ID=1
BASE_GOERLI_SUBSCRIPTION_ID=1
FUJI_SUBSCRIPTION_ID=1
BSC_TESTNET_SUBSCRIPTION_ID=1

# Gas Limits for Chainlink Functions (optional, defaults to 300000)
ETHEREUM_GAS_LIMIT=300000
POLYGON_GAS_LIMIT=300000
ARBITRUM_GAS_LIMIT=300000
OPTIMISM_GAS_LIMIT=300000
BASE_GAS_LIMIT=300000
AVALANCHE_GAS_LIMIT=300000
BNB_GAS_LIMIT=300000

